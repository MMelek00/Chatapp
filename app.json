{
  "expo": {
    "name": "Business App",
    "icon": "./assets/iconios.png",
    "sdkVersion": "30.0.0",
    "slug": "Chatapp",
    "version": "1.0.0",
    "ios": {
      "bundleIdentifier": "com.mousaalharby.businessapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to change your Profile Picture."
      }
    },
    "android": {
      "package": "com.mousaalharby.businessapp"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "loading": {
      "hideExponentText": false,
      "icon": "./assets/loading.png"
    },
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "packagerOpts": {
      "assetExts": ["ttf"],
      "nonPersistent": "--nonPersistent"
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    }
  }
}
